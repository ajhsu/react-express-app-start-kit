{
  "name": "react-express-app",
  "scripts": {
    "dev": "NODE_ENV=development ./node_modules/.bin/gulp watch",
    "build": "NODE_ENV=production ./node_modules/.bin/gulp",
    "start:simple": "node ./src/scripts/server/index.js",
    "start": "npm run pm2:update && ./node_modules/.bin/pm2 start ./src/scripts/server/index.js --watch --name='react-express-app' && npm run logs",
    "stop": "./node_modules/.bin/pm2 delete 'react-express-app' --watch",
    "logs": "./node_modules/.bin/pm2 logs 'react-express-app'",
    "pm2:update": "./node_modules/.bin/pm2 update",
    "test": "./node_modules/.bin/tape ./src/scripts/tests/index.js | ./node_modules/.bin/tap-spec",
    "test:watch": "./node_modules/.bin/tape-watch ./src/scripts/tests/index.js -p tap-spec",
    "test:coverage": "./node_modules/.bin/istanbul cover ./node_modules/.bin/tape ./src/scripts/tests/index.js | ./node_modules/.bin/tap-spec"
  },
  "dependencies": {
    "express": "^4.13.4",
    "http-rewrite-middleware": "^0.1.6",
    "i18next-client": "^1.11.4",
    "lodash": "^3.10.1",
    "mobile-detect": "^1.3.1",
    "page": "^1.7.0",
    "pm2": "^1.0.2",
    "react": "^0.14.7",
    "react-bootstrap": "^0.23.2",
    "react-dom": "^0.14.7",
    "react-redux": "^4.4.5",
    "react-slick": "^0.9.3",
    "react-tap-event-plugin": "^0.2.2",
    "redux": "^3.5.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "babel": "^6.5.2",
    "babel-plugin-syntax-async-functions": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-regenerator": "^6.9.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.9.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "glob": "^7.0.5",
    "gulp": "^3.9.1",
    "gulp-debug": "^2.1.2",
    "gulp-postcss": "^6.1.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-strip-debug": "^1.0.2",
    "gulp-uglify": "^1.5.3",
    "istanbul": "^1.1.0-alpha.1",
    "postcss-hexrgba": "^0.2.0",
    "postcss-import": "^8.0.2",
    "precss": "^1.4.0",
    "rimraf": "^2.5.2",
    "run-sequence": "^1.1.5",
    "tap-spec": "^4.1.1",
    "tape": "^4.5.1",
    "tape-watch": "^2.1.0",
    "vinyl-source-stream": "^1.1.0"
  }
}
